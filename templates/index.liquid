  <script>
    // show links to men's and women's when a user hovers on a product category
    $('.assortment-product.gendered').mouseover(function() {
      $(this).children('.gendered-product-title').hide()
      $(this).css('justify-content', 'center').addClass('fdc')
      $(this).css('opacity', '70%')
      $(this).children('.gendered-product-link').show()
    })
    $('.assortment-product.gendered').mouseleave(function() {
      $(this).css('justify-content', '').removeClass('fdc')
      $(this).css('opacity', '100%')
      $(this).children('.gendered-product-link').hide()
      $(this).children('.gendered-product-title').show()
    })
    // scroll to show more products on arrow click
    let lastAssortmentProduct = document.getElementById('last-assortment-product')
    let firstAssortmentProduct = document.getElementById('first-assortment-product')
    $('.right-arrow-atf').click(function() {
      lastAssortmentProduct.scrollIntoView({behavior: "smooth", block: "nearest"})
      $('.left-arrow-atf').show()
      $(this).removeClass('clickable').css('opacity', '50%')
    })
    $('.left-arrow-atf').click(function() {
      firstAssortmentProduct.scrollIntoView({behavior: "smooth", block: "nearest"})
      $(this).hide()
      $('.right-arrow-atf').addClass('clickable').css('opacity', '100%')
    })
  </script>

