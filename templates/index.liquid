  <script>
    $(document).ready(function() {
      {% comment %} window.setInterval(function () {
        // change active slide
        let currentSlide = $('#homepage-atf-desktop').children('.active')
        let nextSlide = $('#homepage-atf-desktop').children('.active').next()
        currentSlide.removeClass('active').hide()
        // change active bubble
        let currentBubble = $('.col-preview-bubble-container').children('.active')
        let nextBubble = $('.col-preview-bubble-container').children('.active').next()
        currentBubble.removeClass('active')
        nextBubble.addClass('active')
        // go back to slide 1 at then end of the slides
        if (currentSlide.attr('id') == 'slide-3') {
          $('#slide-1').addClass('active').show()
          $('.col-preview-bubble#1').addClass('active')
        } else {
          nextSlide.addClass('active').show()
        }
      }, 4000) {% endcomment %}
    })
    $('.col-preview-bubble').click(function() {
      $('.col-preview-bubble').removeClass('active')
      $('.atf-slide').hide().removeClass('active')
      let selectedSlideId = $(this).attr('id')
      $(this).addClass('active')
      $('#slide-' + selectedSlideId).show().addClass('active')
    })
  </script>

