  <script>
    $(document).ready(function() {
      window.setInterval(function () {
        // change active slide
        {% comment %} $('atf-slide').removeClass('slide-in-slow slide-out-slow') {% endcomment %}
        let currentSlide = $('#homepage-atf-desktop').children('.active')
        let nextSlide = $('#homepage-atf-desktop').children('.active').next('.atf-slide')
        currentSlide.removeClass('active').hide()
        {% comment %} currentSlide.addClass('slide-in-slow')
        currentSlide.removeClass('active slide-in-slow') {% endcomment %}
        {% comment %} setTimeout(function() {
          currentSlide.addClass('slide-out-slow')
        }, 1)
        nextSlide.addClass('active slide-in-slow') {% endcomment %}
        // change active bubble
        let currentBubble = $('.col-preview-bubble-container').children('.active')
        let nextBubble = $('.col-preview-bubble-container').children('.active').next()
        currentBubble.removeClass('active')
        nextBubble.addClass('active')
        // go back to slide 1 at then end of the slides
        if (currentSlide.attr('id') == 'slide-3') {
          $('#slide-1').addClass('active ').show()
          $('.col-preview-bubble#1').addClass('active')
        } else {
          nextSlide.addClass('active').show()
        }
      }, 4000) 
    })
    $('.col-preview-bubble').click(function() {
      $('.col-preview-bubble').removeClass('active')
      $('.atf-slide').hide().removeClass('active')
      let selectedSlideNum = $(this).attr('id')
      let selectedSlideId = $('#slide-' + selectedSlideNum)
      $(this).addClass('active')
      $(selectedSlideId).show().addClass('active')
    })
  </script>

